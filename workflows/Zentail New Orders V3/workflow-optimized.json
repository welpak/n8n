      "parameters": {
        "jsCode": "// Collect all unique SKUs from order products\nconst orderData = $node[\"Prepare Contact Data\"].json.orderData;\nconst products = orderData.products || [];\n\n// Extract all SKUs from kit components\nconst allSKUs = new Set();\nproducts.forEach(product => {\n  if (product.routing_info && product.routing_info.length > 0) {\n    product.routing_info.forEach(routing => {\n      if (routing.kitComponents && routing.kitComponents.length > 0) {\n        routing.kitComponents.forEach(kit => {\n          if (kit.SKU) allSKUs.add(kit.SKU);\n        });\n      }\n    });\n  }\n});\n\nreturn {\n  json: {\n    skus: Array.from(allSKUs),\n    orderData,\n    contactId: $input.item.json.id\n  }\n};"
      },
      "id": "collect-skus",
      "name": "Collect All SKUs",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [2800, 0]
    },
    {
      "parameters": {
        "resource": "custom",
        "customResource": "product.template",
        "operation": "getAll",
        "returnAll": true,
        "options": {
          "fieldsList": ["id", "default_code", "product_variant_id"]
        },
        "filterRequest": {
          "filter": [
            {
              "fieldName": "default_code",
              "operator": "in",
              "value": "={{ $json.skus }}"
            }
          ]
        }
