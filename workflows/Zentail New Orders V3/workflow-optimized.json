      "parameters": {
        "jsCode": "// Prepare contact data\nconst baseData = $input.item.json;\nconst shippingAddress = baseData.shippingAddress || baseData.orderData.shippingAddress;\nconst stateId = baseData.stateId || $node[\"Lookup State\"]?.json?.id || null;\nconst countryId = baseData.countryId;\n\nconst toTitleCase = (str) => {\n  if (!str) return '';\n  return str.toLowerCase().split(' ').map(word => \n    word.charAt(0).toUpperCase() + word.slice(1)\n  ).join(' ');\n};\n\nreturn {\n  json: {\n    contactInfo: {\n      name: toTitleCase(shippingAddress.name),\n      street: shippingAddress.addressLine1,\n      street2: shippingAddress.addressLine2 || '',\n      city: toTitleCase(shippingAddress.city),\n      state_id: stateId,\n      zip: shippingAddress.postalCode,\n      country_id: countryId,\n      email: shippingAddress.email,\n      phone: shippingAddress.phone\n    },\n    orderData: baseData.orderData,\n    isBlacklisted: shippingAddress.name.toLowerCase().includes('beetox')\n  }\n};"
      },
