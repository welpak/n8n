      "parameters": {
        "jsCode": "// Smart State and Country Lookup Logic\nconst shippingAddress = $input.item.json.shippingAddress;\nconst countryCode = shippingAddress.countryCode?.toUpperCase() || '';\nconst stateCode = shippingAddress.state?.replace(/[.,;:!?]/g, '').trim() || '';\n\n// Country IDs that don't require state lookup\nconst NO_STATE_COUNTRIES = [181, 20, 224, 231, 75, 55, 43, 173, 88, 57, 240, 165, 91, 68, 227];\n\n// Common country codes to IDs (most common ones)\nconst COUNTRY_MAP = {\n  'US': 233,\n  'CA': 40,\n  'MX': 156,\n  'GB': 231,\n  'AU': 14,\n  'DE': 59,\n  'FR': 76,\n  'IT': 112,\n  'ES': 68,\n  'NL': 165,\n  'BE': 20,\n  'CH': 214,\n  'AT': 15,\n  'SE': 210,\n  'NO': 167,\n  'DK': 59,\n  'FI': 73,\n  'IE': 106,\n  'NZ': 166,\n  'SG': 198,\n  'JP': 115,\n  'KR': 122,\n  'CN': 47,\n  'IN': 105,\n  'BR': 32,\n  'AR': 11,\n  'CL': 46,\n  'CO': 49,\n  'PE': 174\n};\n\nreturn {\n  json: {\n    countryCode,\n    stateCode,\n    countryId: COUNTRY_MAP[countryCode] || null,\n    needsStateLookup: stateCode.length > 2 && !NO_STATE_COUNTRIES.includes(COUNTRY_MAP[countryCode]),\n    needsCountryLookup: !COUNTRY_MAP[countryCode],\n    shippingAddress,\n    orderData: $input.item.json\n  }\n};"
      },
