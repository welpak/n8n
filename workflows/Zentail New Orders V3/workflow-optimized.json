      "id": "batch-lookup-products",
      "name": "Batch Lookup Products",
      "type": "n8n-nodes-base.odoo",
      "typeVersion": 1,
      "position": [3000, 0],
      "alwaysOutputData": true,
      "credentials": {
        "odooApi": {
          "id": "eCEcpp3pBHncPgxX",
